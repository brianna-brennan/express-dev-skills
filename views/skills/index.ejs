<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Express Dev Skills</title>
</head>
<body>
  <a href="/">Home</a>
  <h1>Skills</h1>
  <a class="new-link" href="/skills/new">Add Skill</a>
  
    <ul>
      <% skills.forEach(function(s) { %>
        <tr>
        <td><a href="/skills/<%= s.id %>"><%= s.skill %></a></td>
        <td><%= s.can ? 'can' : 'cant' %></td>
        <td>
          <form action="/skills/<%= s.id %>?_method=DELETE" method="POST">
            <button type="submit" class="btn btn-danger">X</button>
          </form>
        </td>
      </tr>
    <% }); %>
  </ul>
</body>
</html>